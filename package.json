{
  "name": "miterengel",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev:https": "NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop --https",
    "dev": "gatsby develop",
    "test": "NODE_ENV=production jest --bail --config ./configs/jest/jest.config.js",
    "test:watch": "jest --watchAll --config ./configs/jest/jest.config.js",
    "test:coverage": "jest --coverage --config ./configs/jest/jest.config.js",
    "lint": "npx prettier-eslint --write 'src/**/*.js' && npx prettier-eslint --write 'src/**/*.tsx'",
    "type-check": "tsc --p configs/typescript/tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier-eslint --write",
      "git add",
      "jest --bail --findRelatedTests --config ./configs/jest/jest.config.js"
    ],
    "*.css": [
      "prettier-eslint --write",
      "git add"
    ],
    "{*.{json,md}}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@icons/material": "^0.4.1",
    "@material-ui/core": "^4.4.0",
    "axios": "^0.19.0",
    "gatsby": "^2.15.9",
    "gatsby-image": "^2.2.17",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-layout": "^1.1.6",
    "gatsby-plugin-manifest": "^2.2.14",
    "gatsby-plugin-netlify": "^2.1.11",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.2.20",
    "gatsby-plugin-styled-components": "^3.1.4",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-source-filesystem": "^2.1.21",
    "gatsby-transformer-sharp": "^2.2.13",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.12",
    "react-image-crop": "^8.3.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-remove-graphql-queries": "^2.7.6",
    "babel-preset-gatsby": "^0.2.12",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "^16.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  }
}
